
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - CoreDemo Admin</title>
    <link rel="stylesheet" href="~/writertemp/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/writertemp/assets/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/writertemp/assets/css/style.css">
    <link rel="shortcut icon" href="~/writertemp/assets/images/favicon.ico" />
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        <partial name="_WParcaUst" />
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <partial name="_WParcaSol" />
            <!-- partial -->
            <div class="main-panel">
                <!-- content-wrapper start -->
                @RenderBody()
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <partial name="_WParcaAlt" />
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/writertemp/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/writertemp/assets/vendors/chart.js/Chart.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/writertemp/assets/js/off-canvas.js"></script>
    <script src="~/writertemp/assets/js/hoverable-collapse.js"></script>
    <script src="~/writertemp/assets/js/misc.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="~/writertemp/assets/js/dashboard.js"></script>
    <script src="~/writertemp/assets/js/todolist.js"></script>
    <!-- End custom js for this page -->
</body>
</html>